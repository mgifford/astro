  "peerDependencies": {
    "@types/react": "^17.0.50 || ^18.0.21",
    "@types/react-dom": "^17.0.17 || ^18.0.6",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
